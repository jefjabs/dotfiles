cd ~/
git clone https://github.com/jefjabs/dotfiles.git
ln -s ~/dotfiles/.vim ~/.vim
ln -s ~/dotfiles/.vim ~/.nvim
ln -s ~/dotfiles/.vimrc ~/.vimrc
ln -s ~/dotfiles/.vimrc ~/.nvimrc
ln -s ~/dotfiles/.tmux.conf ~/.tmux.conf 
ln -s ~/dotfiles/.promptline_cyan_overload.sh ~/.promptline_cyan_overload.sh
vim -c "PlugInstall | q! | q!"
ln -s ~/dotfiles/.vim/plugged/vim-colorschemes/colors ~/dotfiles/.vim/colors
